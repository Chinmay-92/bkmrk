{
  "name": "bkmrk-frontend-graphql",
  "version": "1.0.0",
  "description": "GraphQL Frontend for bkmrk.space",
  "scripts": {
    "start": "cross-env PORT=3002 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurekbarth/bkmrk.git"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.0.4",
    "apollo-link": "^1.0.7",
    "apollo-link-context": "^1.0.3",
    "apollo-link-http": "^1.3.2",
    "apollo-link-state": "^0.3.0",
    "flow-bin": "^0.62.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "sanitize.css": "^5.0.0",
    "styled-components": "^2.4.0"
  },
  "keywords": ["bookmarking"],
  "author": "Jurek Barth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jurekbarth/bkmrk/issues"
  },
  "homepage": "https://bkmrk.space",
  "devDependencies": {
    "cross-env": "^5.1.3",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "prettier-eslint": "^8.7.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": ["airbnb", "plugin:import/errors"],
    "plugins": ["react"],
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": 0,
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0,
      "import/resolver": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "react/forbid-prop-types": 0,
      "react/require-default-props": 0
    }
  }
}
